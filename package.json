{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "prebuild": "tsc --noEmit",
    "build": "vite build",
    "test": "mocha --require ts-node/register --require ./mochaSetup.js './src/**/*.test.ts'",
    "start": "vite build && node server.js",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "stylelint": "npx stylelint **/*.scss",
    "precommit": "npm run test && npm run lint && npm run stylelint",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/jsdom": "^21.1.1",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.15",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "chai": "^4.3.7",
    "eslint": "^8.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-no-loops": "^0.3.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "mocha": "^10.2.0",
    "sass": "^1.63.6",
    "sinon": "^15.1.0",
    "stylelint": "^15.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "jsdom": "^22.1.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.2",
    "node-sass": "npm:sass@^1.49.0",
    "stylelint-config-standard": "^33.0.0",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-handlebars": "^1.6.0"
  }
}
